{
  "source": {
    "include": [
      "packages/core"
    ],
    "exclude": [
      "packages/core/src/render/index.js",
      "packages/core/src/render/context.js",
      "packages/core/src/render/directives.js",
      "test/",
      "packages/core/src/utils/",
      "node_modules/",
      "packages/core/dist/"
    ],
    "excludePattern": "node_modules"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "private": false,
    "readme": "./README.md",
    "template": "node_modules/docdash"
  },
  "templates": {
    "default": {
      "outputSourceFiles": false
    }
  },
  "docdash": {
    "sectionOrder": [
      "Classes",
      "Modules",
      "Externals",
      "Events",
      "Namespaces",
      "Mixins",
      "Tutorials",
      "Interfaces"
    ],
    "disqus": "",
    "openGraph": {
      "title": "",
      "type": "website",
      "image": "",
      "site_name": "",
      "url": ""
    },
    "meta": {
      "title": "",
      "description": "",
      "keyword": ""
    },
    "search": true,
    "commonNav": true,
    "collapse": true,
    "navLevel": 3,
    "menu": {
      "Project Website": {
        "href": "https://myproject.com",
        "target": "_blank",
        "class": "menu-item",
        "id": "website_link"
      },
      "Forum": {
        "href": "https://myproject.com.forum",
        "target": "_blank",
        "class": "menu-item",
        "id": "forum_link"
      }
    }
  }
}